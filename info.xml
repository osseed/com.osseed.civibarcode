<?xml version="1.0"?>
<extension key="com.osseed.civibarcode" type="module">
  <file>civibarcode</file>
  <name>Event Registration Barcode</name>
  <description>Generate Barcode for Event registration email</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>OSSeed</author>
    <email>hello@osseed.com</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/osseed/com.osseed.civibarcode</url>
    <url desc="Support">https://github.com/osseed/com.osseed.civibarcode/issues</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2013-09-23</releaseDate>
  <version>1.0</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>5.0</ver>
  </compatibility>
  <comments>Generate Barcode for Event registration email</comments>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/Civibarcode</namespace>
    <format>22.05.2</format>
    <angularModule>crmCivibarcode</angularModule>
  </civix>
  <mixins>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
  </mixins>
</extension>
